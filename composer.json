{
    "name": "skonsoft/wsse-authentication-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to implement WSSE authentication",
    "keywords": [
        "wsse",
        "authentication",
        "bundle"
    ],
    "homepage": "https://github.com/skonsoft/EscapeWSSEAuthenticationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Joos",
            "email": "david.joos@escapestudios.com"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/djoos/EscapeWSSEAuthenticationBundle/graphs/contributors"
        },
        {
            "name": "Mathieu Boldo",
            "email": "mathieu.boldo@entrili.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/cache": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/security-core": "^5.4|^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/finder": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0",
        "phpunit/phpunit": "^10.0",
        "psr/simple-cache": "^3.0",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Escape\\WSSEAuthenticationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Escape\\WSSEAuthenticationBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*|6.*"
        }
    }
}
