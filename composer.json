{
    "name": "skonsoft/wsse-authentication-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to implement WSSE authentication",
    "keywords": [
        "wsse",
        "authentication",
        "bundle"
    ],
    "homepage": "https://github.com/skonsoft/EscapeWSSEAuthenticationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Joos",
            "email": "david.joos@escapestudios.com"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/djoos/EscapeWSSEAuthenticationBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "symfony/cache": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/security-core": "5.0.*",
        "doctrine/common": "^3.0"
    },
    "require-dev": {
        "symfony/finder": "5.0.*",
        "symfony/phpunit-bridge": "5.0.*",
        "phpunit/phpunit": "^8.5",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Escape\\WSSEAuthenticationBundle": ""
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    },
    "target-dir": "Escape/WSSEAuthenticationBundle"
}
